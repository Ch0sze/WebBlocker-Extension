{
        "manifest_version": 3,
        "name": "WebBlocker",
        "version": "1.0.0",
        "description": "Extension to block websites in selected time period",
        "permissions": [
            "activeTab"
        ],
        "content_scripts": [
            {
                "matches": ["<all_urls>"],
                "js": ["js/script.js"]
            }
        ],
        "action": {
            "default_popup": "index.html",
            "default_title": "WebBlocker",
            "default_icon": {
                "16": "images/ad-blocker16.png",
                "32": "images/ad-blocker32.png",
                "128": "images/ad-blocker128.png"
            }
        }
}
